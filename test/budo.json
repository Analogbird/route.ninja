{
	"process": "/middleware/traffic",
	"routes": [
		{
			"get": "/style",
			"run": "style",
			"from": "/controllers/ninja"
		},
		{
			"get": "/weapon",
			"run": "weapon",
			"from": "/controllers/ninja",
			"via": [
				"process.filter",
				"process.validate",
				"process.terminate"
			]
		}
	]
}
