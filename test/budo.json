{
	"pre": "/middleware/prepare",
	"routes": [
		{
			"get": "/style",
			"run": "style",
			"from": "/controllers/ninja"
		},
		{
			"get": "/weapon",
			"run": "weapon",
			"from": "/controllers/ninja",
			"via": [
				"pre.filter",
				"pre.validate"
			]
		},
		{
			"post": "/legend",
			"run": "legend",
			"from": "/controllers/ninja"
		}
	]
}
